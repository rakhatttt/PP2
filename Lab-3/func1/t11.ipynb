    "def is_palindrome(s):\n",
    "    s = s.replace(\" \", \"\").lower()\n",
    "    return s == s[::-1]\n",
    "\n",
    "# Test cases\n",
    "print(is_palindrome(\"madam\"))  # True\n",
    "print(is_palindrome(\"nurses run\"))  # True\n",
    "print(is_palindrome(\"hello\"))  # False"
   ]
  }
