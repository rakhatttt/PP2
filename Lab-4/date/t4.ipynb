   "source": [
    "from datetime import datetime\n",
    "\n",
    "d1 = datetime(2023, 10, 15, 12, 0, 0) \n",
    "d2 = datetime(2023, 10, 10, 10, 30, 0)  \n",
    "\n",
    "dif = d1 - d2\n",
    "\n",
    "dif_s = dif.total_seconds()\n",
    "\n",
    "print(\"Date 1:\", d1)\n",
    "print(\"Date 2:\", d2)\n",
    "print(\"Difference in seconds:\", dif_s)"
   ]
  }
