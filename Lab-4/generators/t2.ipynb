   "source": [
    "def gen(n):\n",
    "    for i in range(0,n+1):\n",
    "        if i%2==0:\n",
    "            yield i\n",
    "\n",
    "def main():\n",
    "    n=int(input(\"Number: \"))\n",
    "    geen=gen(n)\n",
    "    print(\",\".join(map(str,geen)))\n",
    "\n",
    "if __name__==\"__main__\":\n",
    "    main()"
   ]
  }
