    "def gen(n):\n",
    "    for i in range(0,n):\n",
    "        if i%3==0 and i%4==0:\n",
    "            yield i\n",
    "def main():\n",
    "    n=int(input(\"Enter a number:\"))\n",
    "    div=gen(n)\n",
    "    print(list(div))\n",
    "if __name__==\"__main__\":\n",
    "    main()\n"
   ]
