   "source": [
    "def count(a):\n",
    "    u=0\n",
    "    l=0\n",
    "\n",
    "    for x in a:\n",
    "        if x.isupper():\n",
    "            u+=1\n",
    "        elif x.islower(): \n",
    "            l+=1\n",
    "    print(\"Uppercase:\", u)\n",
    "    print(\"Lowercase:\", l)\n",
    "\n",
    "teststring=input()\n",
    "\n",
    "func=count(teststring)\n",
    "print(teststring, func)\n"
   ]
  }
